jQuery(function(){jQuery(":input").on("focus",function(){var a=jQuery(this),c=a.attr("id")||"(no input ID)",d=a.attr("name")||"(no input name)",e=a.attr("class")||"(no input class)",b=jQuery(this.form),f=b.attr("id")||"(no form ID)",g=b.attr("name")||"(no form name)",h=b.attr("class")||"(no form class)";window[gtm4wp_datalayer_name].push({event:'gtm4wp.formElementEnter',inputID:c,inputName:d,inputClass:e,formID:f,formName:g,formClass:h})}).on("blur",function(){var a=jQuery(this),c=a.attr("id")||"(no input ID)",d=a.attr("name")||"(no input name)",e=a.attr("class")||"(no input class)",b=jQuery(this.form),f=b.attr("id")||"(no form ID)",g=b.attr("name")||"(no form name)",h=b.attr("class")||"(no form class)";window[gtm4wp_datalayer_name].push({event:'gtm4wp.formElementLeave',inputID:c,inputName:d,inputClass:e,formID:f,formName:g,formClass:h})})})